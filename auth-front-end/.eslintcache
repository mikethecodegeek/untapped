ired\n        /> */}\n        \n        <ReactStars\n          count={5}\n          className='checkinRating'\n          onChange={changeRating}\n          size={24}\n          activeColor=\"#ffd700\"\n        />\n     \n     \n      <button className='confirmButton' type=\"submit\">Confirm</button>\n    </form>\n  );\n}\n\nexport default CheckinPage;","/home/mike/AppAcademy/w15/tryagain/auth-front-end/src/store/checkin.js",[],"/home/mike/AppAcademy/w15/tryagain/auth-front-end/src/components/Checkin/index.js",["141","142"],"import {useSelector,useDispatch} from 'react-redux';\nimport {useEffect} from 'react'\nimport {getCheckin} from '../../store/checkin'\nimport {useParams} from 'react-router-dom'\n\n\nconst Checkin = () => {\n    const dispatch = useDispatch();\n    const { id } = useParams();\n    const checkins = useSelector(state => state.checkins);\n    console.log(checkins)\n    useEffect(()=>{\n        dispatch(getCheckin(id))\n    },[])\n\n    return (\n        <div>\n            <h2>Checkin</h2>\n            { checkins && \n                checkins.map(checkin=>{\n                    if (checkin.User && checkin.Beer && checkin.Brewery) {\n\n                        return <div>\n                            <p>{checkin.User.username}</p>\n                            <p>{checkin.Beer.name}</p>\n                            <p>{checkin.Brewery.name}</p>\n                            <p>{checkin.rating}</p>\n                        </div>\n                    }\n                })\n            }\n\n        </div>\n    )\n}\n\nexport default Checkin;","/home/mike/AppAcademy/w15/tryagain/auth-front-end/src/components/Profile/index.js",["143","144"],"import { useSelector, useDispatch } from \"react-redux\";\nimport { useEffect } from \"react\";\nimport { getUserCheckins } from \"../../store/checkin\";\nimport { useParams } from \"react-router-dom\";\nimport UserCheckin from \"../../components/UI/UserCheckin\";\nimport \"./profile.css\";\n\nconst Profile = () => {\n 